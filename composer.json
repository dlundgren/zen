{
    "name": "woohoolabs/zen",
    "description": "Woohoo Labs. Zen DI Container",
    "type": "library",
    "keywords": ["Woohoo Labs.", "Zen", "DI", "DIC", "Ioc", "Dependency Injection Container", "PSR-11"],
    "license": "MIT",
    "authors": [
        {
            "name":"Máté Kocsis",
            "email":"kocsismate@woohoolabs.com"
        }
    ],
    "support": {
        "issues": "https://github.com/woohoolabs/zen/issues",
        "source": "https://github.com/woohoolabs/zen"
    },
    "require": {
        "php":"^7.0.0",
        "psr/container": "^1.0.0",
        "doctrine/annotations": "^1.4.0",
        "php-di/phpdoc-reader": "^2.0.1"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^6.0.0",
        "squizlabs/php_codesniffer": "^2.8.0"
    },
    "provide": {
        "psr/container-implementation": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "WoohooLabs\\Zen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WoohooLabs\\Zen\\Examples\\": "examples/",
            "WoohooLabs\\Zen\\Tests\\Unit\\": "tests/unit"
        }
    },
    "bin": [
        "bin/zen"
    ],
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "bin-compat": "full",
        "platform": {
            "php": "7.0"
        }
    }
}
